<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="@dimen/default_dialog_width"
    android:layout_height="@dimen/default_dialog_height"
    android:background="@drawable/dialog_background"
    android:gravity="center"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="@string/contact_thanks_title"
        android:textColor="#333333"
        android:textSize="13.6dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="6.4dp"
        android:text="我们很重视您的意见，您的意见"
        android:textColor="#333333"
        android:textSize="9.6dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:text="是我们前进的动力。"
        android:textColor="#333333"
        android:textSize="9.6dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView3"
        android:textColor="#e5e5e5" />

    <TextView
        android:id="@+id/button1"
        android:layout_width="match_parent"
        android:layout_height="35.2dp"
        android:layout_alignParentBottom="true"
        android:gravity="center"
        android:text="@string/default_ensure"
        android:textColor="#1f76dc"
        android:textSize="12dp" />

</RelativeLayout>